{
  "title": "Upper Body Push",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "allOf": [
    {
      "$ref": "/schema/mens_health.json"
    },
    {
      "properties": {
        "title": {
          "type": "string",
          "default": "Upper Body Push"
        },
        "source": {
          "type": "string",
          "default": "http://www.menshealth.com/fitness/tuesday-workout-upper-body"
        },
        "workout": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "enum": [
                  "Barbel Pause Bench Press",
                  "Dumbbell Single-Arm Overhead Press",
                  "Tempo Pushup",
                  "Bear Crawl"
                ]
              },
              "weight": {
                "type": "integer"
              },
              "reps": {
                "type": "integer"
              },
              "time": {
                "type": "string"
              },
              "notes": {
                "type": "string"
              }
            },
            "required": [
              "name",
              "weight",
              "time",
              "notes"
              ]
          }
        }
      }
    }
  ]
}
